.wrapper{
  width: 100%;
  height: 100%;
  position: relative;
}
.content{
  position: absolute;
  left: 0;
  top: 0;
}
.bg{
  width: 100%;
  height: 1206rpx;
}
.bg image{
  width: 100%;
  height: 100%;
}
.header{
  text-align: center;
  width: 690rpx;
  margin-left: 35rpx;
  height: 315rpx;
  background-color: rgba(73, 147, 250, .6);
  border-radius: 10rpx;
}
.header_img{
  width: 133rpx;
  height: 133rpx;
  border-radius: 50%;
  margin-left: 275rpx;
  padding-top: 25rpx;
}
.header_img image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.name{
  margin-top: 30rpx;
  font-size: 36rpx;
  color: #fff;
}
.school{
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #fff;
}
.center{
  width: 690rpx;
  margin-left: 35rpx;
  height: 730rpx;
  background-color: rgba(73, 147, 250, .6);
  border-radius: 10rpx;
  margin-top: 10rpx;
}
.center_title{
  width: 100%;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 34rpx;
  color: #fff;
  padding-top: 30rpx;
}
.center_content{
  width: 100%;
  margin-top: 60rpx;
}
.list{
  width: 100%;
  padding-left: 75rpx;
  height: 170rpx;
}
.last{
  margin-right: 0;
}
.list_li{
  width: 100rpx;
  height: 170rpx;
  float: left;
  margin-right: 110rpx;
  margin-bottom: 25rpx;
}
.list_img{
  width: 100rpx;
  height: 100rpx;
}
.list_img image{
  width: 100%;
  height: 100%;
}
.list_name{
  width: 100%;
  height: 70rpx;
  text-align: center;
  margin-top: 25rpx;
  font-size: 34rpx;
  color: #fff;
}
.last_list{
  padding-left:  75rpx;
}
.last_li{
  width: 265rpx;
}
.last_li .list_name{
  text-align: left;
}
button{
  width: 690rpx;
  margin-left: 35rpx;
  height: 90rpx;
  font-size: 34rpx;
  color: #fff;
  background-color: rgba(73, 147, 250, .6);
  margin-top: 10rpx;
}